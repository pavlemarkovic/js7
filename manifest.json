{
  "name": "Cysecor Google",
  "description": "Google postaje Cysecor google",
  "version": "1.0",
  "manifest_version": 3,
  "icon":{
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.google.com/*"],
      "css": ["main.css"],
      "js": ["main.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://*.google.com/*"],
      "resources": ["images/cysecor_logo.png"]
    }
  ]
}